{
    "manifest_version": 3,
    "name": "Minesweeper game",
    "version": "1.0",
    "description": "Play the classic Minesweeper puzzle game!",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "48": "smile.png"
      }
    },
    "icons": {
      "48": "smile.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["smile.png"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  